        <corners android:radius="2.0dip" />
        <solid android:color="@color/color_login_btn_sel" />
    </shape>
</item>
<item android:state_pressed="true">
    <shape>
        <corners android:radius="2.0dip" />
        <solid android:color="@color/color_login_btn_sel" />
    </shape>
</item>
<item android:state_selected="true">
    <shape>
        <corners android:radius="2.0dip" />
        <solid android:color="@color/color_login_btn_sel" />
    </shape>
</item>
<item>
    <shape>
        <corners android:radius="2.0dip" />
        <solid android:color="@color/white" />
    </shape>
</item>
</selector>